{
  "name": "ember-freestyle",
  "version": "0.17.0",
  "description": "Create a living styleguide for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/chrislopresto/ember-freestyle.git",
  "license": "MIT",
  "author": "Chris LoPresto",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "compile-styles": "node ./script/compile-styles.js",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:css": "stylelint \"./**/*.scss\"",
    "lint:css:fix": "stylelint \"./**/*.scss\" --fix",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepare": "husky install",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "release-it",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.4",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-auto-import": "^2.4.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-typescript": "^5.1.1",
    "ember-focus-trap": "^1.0.1",
    "ember-modifier": "^3.2.7",
    "ember-named-blocks-polyfill": "^0.2.5",
    "ember-truth-helpers": "^3.1.1",
    "json-formatter-js": "^2.3.4",
    "macro-decorators": "^0.1.2",
    "strip-indent": "^3.0.0",
    "tracked-built-ins": "^3.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glint/core": "^0.9.4",
    "@glint/environment-ember-loose": "^0.9.4",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "@tsconfig/ember": "^1.0.1",
    "@types/ember": "^4.0.1",
    "@types/ember-qunit": "^5.0.2",
    "@types/ember-resolver": "^5.0.11",
    "@types/ember__application": "^4.0.2",
    "@types/ember__array": "^4.0.2",
    "@types/ember__component": "^4.0.10",
    "@types/ember__controller": "^4.0.1",
    "@types/ember__debug": "^4.0.1",
    "@types/ember__engine": "^4.0.2",
    "@types/ember__error": "^4.0.0",
    "@types/ember__helper": "^4.0.0",
    "@types/ember__modifier": "^4.0.2",
    "@types/ember__object": "^4.0.4",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.10",
    "@types/ember__runloop": "^4.0.1",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.0.9",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.8.1",
    "@types/ember__utils": "^4.0.1",
    "@types/qunit": "^2.19.3",
    "@types/remarkable": "^2.0.3",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.40.1",
    "all-contributors-cli": "^6.24.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.7.0",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-code-snippet": "^3.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.7.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-style-modifier": "^0.8.0",
    "ember-template-imports": "^3.3.1",
    "ember-template-lint": "^4.14.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.35.0",
    "eslint-config-ember": "0.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "husky": "^8.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.10.0",
    "remarkable": "^2.0.1",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "volta": {
    "node": "14.19.3",
    "yarn": "1.22.19"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://chrislopresto.github.io/ember-freestyle/",
    "versionCompatibility": {
      "ember": ">= 2.4.0"
    },
    "after": [
      "ember-named-blocks-polyfill"
    ]
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "hooks": {
      "after:release": "ember deploy production"
    }
  }
}
