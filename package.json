{
  "name": "ember-freestyle",
  "version": "0.11.10",
  "description": "Create a living styleguide for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/chrislopresto/ember-freestyle.git",
  "license": "MIT",
  "author": "Chris LoPresto",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "lint:js": "eslint .",
    "lint:scss": "sass-lint .",
    "release": "release-it",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "broccoli-flatiron": "0.1.3",
    "broccoli-merge-trees": "^3.0.1",
    "ember-cli-babel": "^7.13.0",
    "ember-cli-htmlbars": "^4.2.0",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-showdown": "^4.1.0",
    "ember-runtime-enumerable-includes-polyfill": "^2.0.0",
    "glob": "^7.1.3",
    "sass": "^1.17.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "all-contributors-cli": "^6.14.1",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.15.2",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-page-object": "^1.17.0",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sass-lint": "^1.0.3",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.3",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-config-ember": "0.3.0",
    "eslint-plugin-ember": "^7.7.1",
    "eslint-plugin-node": "^10.0.0",
    "loader.js": "^4.2.3",
    "qunit-dom": "^0.9.2",
    "release-it": "^13.6.0",
    "release-it-lerna-changelog": "^2.3.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://chrislopresto.github.io/ember-freestyle/",
    "versionCompatibility": {
      "ember": ">= 2.4.0"
    }
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "hooks": {
      "after:release": [
        "echo Deploying...",
        "ember deploy production"
      ]
    }
  }
}
