{
  "name": "ember-freestyle",
  "version": "0.13.2",
  "description": "Create a living styleguide for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/chrislopresto/ember-freestyle.git",
  "license": "MIT",
  "author": "Chris LoPresto",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "compile-styles": "node ./script/compile-styles.js",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:css": "stylelint \"./**/*.scss\"",
    "lint:css:fix": "stylelint \"./**/*.scss\" --fix",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepare": "husky install",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prerelease": "./script/prerelease",
    "release": "./script/release",
    "postrelease": "./script/postrelease",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "ember-auto-import": "^1.11.3",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-modifier": "^3.0.0",
    "ember-named-blocks-polyfill": "^0.2.4",
    "ember-truth-helpers": "^3.0.0",
    "json-formatter-js": "^2.3.4",
    "macro-decorators": "^0.1.2",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.47.2",
    "all-contributors-cli": "^6.14.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.4",
    "ember-cli-autoprefixer": "^1.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-showdown": "^6.0.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.6",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.14.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-ember": "0.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.1.0",
    "husky": "^7.0.4",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.16.0",
    "qunit-dom": "^2.0.0",
    "remarkable": "^2.0.0",
    "sass": "^1.45.0",
    "standard-version": "^9.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://chrislopresto.github.io/ember-freestyle/",
    "versionCompatibility": {
      "ember": ">= 2.4.0"
    },
    "after": [
      "ember-named-blocks-polyfill"
    ]
  },
  "volta": {
    "node": "12.22.6",
    "yarn": "1.22.15"
  }
}
