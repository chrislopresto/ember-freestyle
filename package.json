{
  "name": "ember-freestyle",
  "version": "0.11.5",
  "description": "Create a living styleguide for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/chrislopresto/ember-freestyle.git",
  "license": "MIT",
  "author": "Chris LoPresto",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "prerelease": "./script/prerelease",
    "release": "./script/release",
    "postrelease": "./script/postrelease"
  },
  "dependencies": {
    "broccoli-flatiron": "0.1.3",
    "broccoli-merge-trees": "^3.0.1",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-showdown": "^4.1.0",
    "ember-runtime-enumerable-includes-polyfill": "^2.0.0",
    "glob": "^7.1.3",
    "sass": "^1.17.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.2.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-github-pages": "0.2.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-page-object": "^1.15.0",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-release": "v1.0.0-beta.2",
    "ember-cli-sass-lint": "^1.0.3",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.3",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.2.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^1.2.1",
    "eslint-config-ember": "0.3.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.2.3",
    "qunit-dom": "^0.6.2",
    "standard-version": "^7.0.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://chrislopresto.github.io/ember-freestyle/",
    "versionCompatibility": {
      "ember": ">= 2.4.0"
    }
  }
}
